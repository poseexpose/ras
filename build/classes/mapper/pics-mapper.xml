<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="dao.pics.PicsDao">
  	<select id="queryTopPic" parameterType="entity.House" resultType="entity.Pics">
  		select * FROM Pics where houseID = #{houseID} limit 1  ;
  	</select>
  </mapper>